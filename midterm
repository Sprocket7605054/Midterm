class Student:
     def __init__(self, name, atten, dress, safety, cell ):
        self.name = name
        self.atten = atten
        self.dress = dress
        self.safety = safety
        self.cell = cell

     def assign_Atten(self, atten):
        self.atten = atten

     def assign_Dress(self, dress):
        self.dress = dress

     def assign_Cell(self, safety):
        self.safety = safety

     def assign_saftry(self, cell):
        self.cell = cell

     def display_properties(self):
        print("Properties of the User:")
        print("Name:", self.name)
        print("Days missed:", self.atten)
        print("Days not in dress code:", self.dress)
        print("Days when they are not safe:", self.safety)
        print("Cellphone usage:", self.cell)

students= ["james", "adam", "cole", "brian", "david", "john", "chandler", "pearson", "dwane", "gavin", "ryan", "shahood", "chris", "aiden", "jaiden", "leo", "marcos", "muntag", "robert", "jacob"]
Class = []

# Creating a user
james = Student(students[0], 0, 0, 0, 0)
#display user properties
james.display_properties()


for x in range(11):
   print(students,x)
   student= students.pop(0)
   Names = Student(students[x])
   Class.insert(1,Names)
   print(students,x)

